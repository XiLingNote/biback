<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bi.baiqiu.mapper.DailyTargetDao">

	<!-- 查询最早日期-->
	<select id="queryDailyTargetTest"  resultType="bi.baiqiu.pojo.DailyTarget">
		SELECT * FROM dailytarget 
	</select>
	<!-- 批量增加日计划 -->
	<insert id="insertByBatch" parameterType="java.util.List"  useGeneratedKeys="true" >
        insert into dailytarget (storename, date,payment)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.storename}, #{item.date},#{item.payment})
        </foreach>
    </insert>
</mapper>